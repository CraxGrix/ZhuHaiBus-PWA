        </v-flex>\n                <v-flex>\n                  <div class=\"caption\">最早班次:{{ router.BeginTime }}</div>\n                  <div class=\"caption\">最后班次:{{ router.EndTime }}</div>\n                  <div class=\"caption\">班次间隔:{{ router.Interval }}min</div>\n                </v-flex>\n              </v-layout>\n            </v-card-title>\n            <v-divider></v-divider>\n          </v-card>\n        </v-list>\n      </v-card>\n    </v-card>\n  </div>\n</template>\n<script>\nimport { mapActions, mapGetters, mapState, mapMutations } from \"vuex\";\nimport { characterMatchRoute } from \"@/script/utils\";\nexport default {\n  data: () => {\n    return {\n      inputValue: null,\n      matchedRouters: []\n    };\n  },\n  computed: {\n    ...mapState([\"searchBarStatus\", \"routers\"]),\n    formatRouterInfo() {\n      return this.matchRouters.reduce((acc, cur) => ((acc[cur.Id] = cur) && acc), {})\n    }\n  },\n  watch: {\n    inputValue(val) {\n      this.matchedRouters = characterMatchRoute(`${val}路`);\n    }\n  },\n  methods: {\n    ...mapActions([\"changeSearchBarState\", \"viewConversion\"]),\n    ...mapMutations([\"addRouters\", \"stickyRouters\"]),\n    createRouterCard(id) {\n      this.changeSearchBarState();\n      this.sortRouters(id);\n      if (this.check(id)) {\n        this.stickyRouters(id);\n      } else {\n        this.addRouters({\n          ...this.formatRouterInfo(),\n          start: id,\n          end: this.formatRouterInfo()\n        });\n      }\n      this.viewConversion({\n        name: \"details\",\n        params: this.matchedRouters.find(r => r[\"Id\"] === id)\n      });\n    },\n    sortRouters(id) {\n      this.matchedRouters =\n        this.matchedRouters[0][\"Id\"] === id\n          ? this.matchedRouters\n          : this.matchedRouters.reverse();\n    },\n    check(id) {\n      return this.routers.some(info => info.hasOwnProperty(id));\n    }\n  }\n};\n</script>\n<style>\n.modal-backdrop {\n  position: fixed;\n  top: 0;\n  right: 0;\n  bottom: 0;\n  left: 0;\n  background-color: rgba(0, 0, 0, 0.3);\n  z-index: 999;\n}\n.search_modal_wrap {\n  position: absolute;\n  display: flex;\n  flex-direction: column;\n  top: 2.09vh;\n  left: 2.09vh;\n  right: 2.09vh;\n}\n.v-list {\n  padding: 0;\n}\n.v-text-field {\n  padding-top: 5px;\n}\n.search_bar {\n  display: flex;\n}\n.search_result_wrap {\n  height: 6.53vh;\n}\n.arrow_forward_icon {\n  position: relative;\n  bottom: 0.32vh;\n}\n\n.router_time_info_container {\n}\n</style>\n\n"],"sourceRoot":"src/components"}]}